{% extends 'base.html.twig' %}

{% block title %}
    {{ 'title.user_edit'|trans }}
{% endblock %}

{% block body %}
    <div class="container">
        <div class="row">
            {% include 'default/_form.html.twig' with {
                page_title: 'title.user_edit'|trans({'%id%': user.id|default('')}),
                form: form,
                submit_label: 'action.edit'|trans,
                back_to_list_path: 'user_index',
            } %}
        </div>
    </div>
    {%  include 'footer.html.twig' %}
{% endblock %}